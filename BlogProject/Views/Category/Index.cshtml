@using BlogProject.Entity.Concrete
@model List<Category>
@{
    Layout = null;
}

<table>
    <tr>
        <th>ID</th>
        <th>Kategori Adı</th>
        <th>Durum</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.CategoryID</td>
        <td>@item.CategoryName</td>
        <td>@item.CategoryStatus</td>
        <td>@item.CategoryDescription</td>
    </tr>
    }
</table>